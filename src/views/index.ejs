      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

<!-- Header start -->
<div class="navbar custom-navbar wow fadeInDown" data-wow-duration="2s" role="navigation" id="header">
  <div class="header">
    <div class="container"> 
      <!-- Row Rtart -->
      <div class="row">
          <% if( articels.site_main_logo.url != ''){ %>
        <div class="col-md-3 col-sm-3">
          <div class="logo"><img src="<%- articels.site_main_logo.url %>" alt="<%- articels.site_main_logo.alt %>"></div>
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
          </div>
        </div>
        <% } %>
        <div class="col-md-9 col-sm-9"> 
          <!--navegation-->
          <div class="navigationwrape">
            <div class="navbar navbar-default" role="navigation">
              <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                  <li> <a href="#home" class="smoothScroll"> Home</a></li>
                  <li> <a href="#about" class="smoothScroll"> About Us</a></li>
                  <li> <a href="#service" class="smoothScroll"> Service</a></li>
                  <li> <a href="#porfolio" class="smoothScroll"> porfolio</a></li>
                  <li> <a href="#price" class="smoothScroll"> Pricing</a></li>
                  <li> <a href="#testimonials" class="smoothScroll"> Testimonials </a></li>
                  <li> <a href="#blog" class="smoothScroll"> Blog</a></li>
                  <li> <a href="#contact" class="smoothScroll"> Contact us </a></li>
                </ul>
              </div>
              <div class="clearfix"></div>
            </div>
          </div>
        </div>
      </div>
      <!-- Row End --> 
    </div>
  </div>
</div>
<!-- Header end --> 

<!-- Slider Start -->
<div class="slider-wrap staticSlide" id="home">
  <div class="container">
    <div class="banner-info">
        <% if( articels.banner_title != ''){ %>
      <div class="banner-head"><%- articels.banner_title %></div>
      <% } %>
      <% if( articels.banner_heading != ''){ %>
      <h2><%- articels.banner_heading %></h2>
      <% } %>
      <%- articels.banner_text %>
      <% if( articels.banner_button.title != ''){ %>
      <div class="readmore"><a href="<%- articels.banner_button.url %>"><%- articels.banner_button.title %></a></div>
      <% } %>
    </div>
  </div>
</div>
<!-- Slider End --> 


<!--Welcome Start-->
<div class="welcome-wrap" id="about">
  <div class="container">
    <div class="col-md-6 pull-right">
      <div class="welcome-info welcome-right">
        <% if( articels.about_us_heading != ''){ %>
        <h1><%- articels.about_us_heading %></h1>
        <% } %>
        <%- articels.about_us_text %>
        <% if( articels.about_us_button.title != ''){ %>
        <div class="readmore"><a href="<%- articels.about_us_button.url %>"><%- articels.about_us_button.title %></a></div>
        <% } %>
      </div>
    </div>
    <% if( articels.about_us_image.url != ''){ %>
    <div class="col-md-6">
      <div class="welcomeImg"><img src="<%- articels.about_us_image.url %>"></div>
    </div>
    <% } %>
    <div class="clearfix"></div>
    <br>
  </div>
</div>
<!--Welcome End--> 

<!--cretive start-->
<div class="cretive-wrap" id="service">
  <div class="container">
    <% if( articels.services_heading != ''){ %>
    <h1><%- articels.services_heading %></h1>
    <% } %>
    <p><%- articels.services_subheading %></p>
    <ul class="row iconsWrp">
      <% articels.services_items.forEach(element => { %>
        <li class="col-md-4 col-sm-4 col-xs-12">
            <div class="icon-box iconbox-theme-colored"> <a class="icon-wrap icon-dark icon-circled icon-border-effect effect-circled icon-lg" href="#"> <%- element.service_icon %></a>
            <h6><%- element.service_title %></h6>
            <p><%- element.service_text %></p>
            </div>
        </li>
      <% }) %>
    </ul>
  </div>
</div>
<!--cretive start--> 

<!--Why Choose Us Start-->
<div class="whychoose-wrap" id="whychoose">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <% if( articels.choose_us_heading != ''){ %>
        <h1><%- articels.choose_us_heading %></h1>
        <% } %>
        <%- articels.choose_us_text %>
        <% if( articels.choose_us_button.title != ''){ %>
        <div class="readmore"><a href="<%- articels.choose_us_button.url %>"><%- articels.choose_us_button.title %></a></div>
        <% } %>
      </div>
    </div>
  </div>
</div>
<!--Why Choose Us End--> 

<!--Portfolio Start-->
<div class="porfolio-wrap" id="porfolio">
  <div class="sortable-masonry">
    <div class="container">
        <% if( articels.portfolio_heading != ''){ %>
      <h1><%- articels.portfolio_heading %></h1>
      <% } %>
      <ul class="row default-portfolio-item">
        <% articels.portfolio_items.forEach(element => { %>
        <li class="col-md-4 col-sm-6">
          <div class="inner-box">
            <div class="image-box"><img src="<%- element.portfolio_image.url %>" alt="<%- element.portfolio_image.alt %>"></div>
            <!--Overlay Box-->
            <div class="overlay-box">
              <div class="overlay-inner">
                <h3><a href="#"><%- element.portfolio_title %></a></h3>
              </div>
            </div>
          </div>
        </li>
        <% }) %>

      </ul>
    </div>
  </div>
</div>
<!--Portfolio End--> 

