<!--Contact Start-->

<div class="contact-wrap" id="contact">
  <div class="container">
    <% if( articels.contact_heading != ''){ %>
        <h1><%- articels.contact_heading %></h1>
        <% } %>
        <% if( articels.contact_subheading != ''){ %>
    <p><%- articels.contact_subheading  %></p>
    <% } %>
    <% if( articels.contact_us_text != ''){ %>
    <div class="call"><%- articels.contact_us_text %></div>
    <% } %>
    <form>
      <div class="row formwrap">
        <div class="col-md-3">
          <div class="input-group">
            <input type="text" name="name" placeholder="Your Name *" class="form-control">
          </div>
        </div>
        <div class="col-md-3">
          <div class="input-group">
            <input type="text" name="email" placeholder="Your Email *" class="form-control">
          </div>
        </div>
        <div class="col-md-3">
          <div class="input-group">
            <input type="text" name="phone" placeholder="Phone No *" class="form-control">
          </div>
        </div>
        <div class="col-md-3">
          <div class="input-group">
            <input type="text" name="date" placeholder="Subject *" class="form-control">
          </div>
        </div>
        <div class="col-md-12">
          <div class="input-group">
            <textarea placeholder="Message *" class="form-control"></textarea>
          </div>
        </div>
        <div class="col-md-12">
          <div class="input-group">
            <input type="submit" class="btn" value="Contact Us Now">
          </div>
        </div>
      </div>
    </form>
  </div>
</div>
<!--Contact End--> 

