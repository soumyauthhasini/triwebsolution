<!-- Testimonials Start -->
<div class="testimonialsWrap" id="testimonials">
  <div class="container">
    <% if( articels.testimonial_heading != ''){ %>
        <h1><%- articels.testimonial_heading %></h1>
        <% } %>
    <ul class="testimonialsList">
      <!-- Client -->
      <% articels.testimonial_items.forEach(element => { %>
      <li class="item">
        <div class="testi-info">
          <div class="testimg"><img src="<%- element.client_image %>" alt="Your alt text here" /></div>
          <div class="clientname">- <%- element.client_name %> -</div>
          <p><%- element.client_message %></p>
        </div>
      </li>
      <% }) %>
      
     
    </ul>
  </div>
</div>
<!-- Testimonials end --> 

